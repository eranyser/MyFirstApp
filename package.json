{
  "name": "angular-app-base",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "tsfmt": "tsfmt -r",
    "tsfmt:changed": "gulp tsfmt && exit 0",
    "commit": "commit-wizard",
    "ngc": "ngc -p ./tsconfig.aot.json",
    "clean": "npm cache clean && rimraf node_modules doc typings coverage dist dll .awcache",
    "clean:dist": "rimraf dist",
    "clean:dev": "rimraf coverage dll .awcache",
    "preclean:install": "npm run clean",
    "clean:install": "yarn install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:test": "npm run test -- --auto-watch --no-single-run --coverage=false",
    "watch:prod": "npm run build:prod -- --watch",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack  --env.env development --progress ",
    "build:prod:local": "webpack  --env.env production --watch",
    "build:prod": "webpack --env.CLIENT_BASE_URL=webclient/ --env.env=production",
    "server:dev": "webpack-dev-server  --env.env development --progress  --watch --content-base src/",
    "server:debug": "webpack-dev-server  --env.env development --progress  --watch --content-base src/ --env.LOG_LEVEL=*7",
    "server:dev:hmr": "npm run server:dev --  --inline --hot",
    "server:prod": "http-server dist --cors",
    "lint": "tslint \"src/**/*.ts\"  -e \"src/custom-typings.d.ts\"",
    "preci": "npm run lint",
    "ci": "npm test",
    "test": "cross-env NODE_ENV=test karma start",
    "docs": "typedoc --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "start": "npm run server:dev",
    "start:hmr": "npm run server:dev:hmr"
  },
  "license": "UNLICSENCED",
  "dependencies": {
    "@angular/animations": "~4.0.0-rc.3",
    "@angular/common": "~4.0.0-rc.3",
    "@angular/compiler": "~4.0.0-rc.3",
